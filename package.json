{
    "name": "common-mpvue",
    "version": "0.3.4",
    "description": "基于mpvue小程序的通用能力封装",
    "main": "index.js",
    "types": "types/index.d.ts",
    "repository": "ssh://git@github.com:thundernet8/common-mpvue.git",
    "author": "wuxueqian <wuxueqian2010@hotmail.com>",
    "license": "MIT",
    "keywords": [
        "wechat",
        "wechat-miniprogram",
        "mpvue"
    ],
    "scripts": {
        "build": "rimraf lib && mkdirp lib && tsc",
        "prepublishOnly": "npm run build",
        "lint": "npm run lint:ts",
        "lint:ts": "tslint -e node_modules -c .tslintrc.json src/**/*.ts",
        "lint-staged": "lint-staged",
        "lint-staged:ts": "tslint --fix -c .tslintrc.json",
        "lint-staged:format": "prettier --write"
    },
    "lint-staged": {
        "src/**/*.{ts,tsx}": [
            "lint-staged:format",
            "lint-staged:ts",
            "git add"
        ]
    },
    "pre-commit": "lint-staged",
    "devDependencies": {
        "@types/node": "^9.6.1",
        "lint-staged": "6",
        "mkdirp": "^0.5.1",
        "pre-commit": "^1.2.2",
        "prettier": "^1.11.1",
        "rimraf": "^2.6.2",
        "tslint": "^5.9.1",
        "tslint-eslint-rules": "^5.1.0",
        "typescript": "^2.8.1",
        "vue": "^2.5.16"
    },
    "dependencies": {
        "left-pad": "^1.2.0",
        "md5": "^2.2.1",
        "mpvue": "^1.0.7",
        "querystring": "^0.2.0",
        "url-parse": "^1.2.0",
        "vuex": "^3.0.1"
    }
}
